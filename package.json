{
  "name": "surabayajs.org",
  "description": "Landing page for SurabayaJS üêä",
  "version": "6.0.0",
  "repository": "https://github.com/surabayajs/surabayajs.org.git",
  "author": "Griko Nibras <griko@surabayajs.org>",
  "scripts": {
    "now-build": "run-s \"build:**\"",
    "build": "env-cmd run-s \"build:**\"",
    "build:scripts": "node scripts/build.js",
    "build:web": "next build && next-sitemap",
    "clean": "rimraf \".next\" \"public/!(.gitkeep)\"",
    "codegen": "env-cmd yarn graphql-codegen --config codegen.yml",
    "develop": "next dev",
    "format": "run-p \"format:**\"",
    "format:index": "prettier --write \"*.{js,json,md}\"",
    "format:src": "prettier --write \"src/**/*.{css,js,ts,tsx}\"",
    "lint": "eslint --cache --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "validate": "run-s lint type-check"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.0.1",
    "@emotion/react": "^11.1.1",
    "@emotion/styled": "^11.0.0",
    "chakra-ui-markdown-renderer": "^1.1.0",
    "chrome-aws-lambda": "^5.5.0",
    "date-fns": "^2.16.1",
    "favicons": "^6.2.0",
    "framer-motion": "^2.9.4",
    "graphql": "^15.4.0",
    "graphql-request": "^3.3.0",
    "litkey": "^0.1.2",
    "next": "^10.0.3",
    "next-seo": "^4.17.0",
    "nprogress": "^0.2.0",
    "puppeteer-core": "^5.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.1.0",
    "react-indiana-drag-scroll": "^1.8.0",
    "react-markdown": "^5.0.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.19.3",
    "@graphql-codegen/typescript": "^1.18.1",
    "@types/node": "^14.14.10",
    "@types/nprogress": "^0.2.0",
    "@types/puppeteer": "^5.4.0",
    "@types/puppeteer-core": "^2.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "copy": "^0.3.2",
    "env-cmd": "^10.1.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-parts": "^1.0.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "next-sitemap": "^1.3.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "puppeteer": "^5.5.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2"
  },
  "license": "MIT"
}
